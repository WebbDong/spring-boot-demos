<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="consoleAppender" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date [%thread] %-5level %logger{80} %L - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="debug">
        <appender-ref ref="consoleAppender" />
    </root>
</configuration>